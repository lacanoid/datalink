create table sample_datalinks (
  url text,
  link datalink
) with oids;
NOTICE:  DATALINK DDL: CREATE TRIGGER "~RI_DatalinkTrigger" BEFORE INSERT OR UPDATE OR DELETE ON sample_datalinks FOR EACH ROW EXECUTE PROCEDURE datalink.dl_trigger_ri();CREATE TRIGGER "~RI_DatalinkTrigger2" BEFORE TRUNCATE ON sample_datalinks FOR EACH STATEMENT EXECUTE PROCEDURE datalink.dl_trigger_ri();
select * from dl_triggers;
 owner |         regclass          | links | mco |       tgname        
-------+---------------------------+-------+-----+---------------------
 root  | datalink.sample_datalinks |     1 |  33 | ~RI_DatalinkTrigger
 root  | sample_datalinks          |     1 |   0 | ~RI_DatalinkTrigger
(2 rows)

select column_name,lco FROM datalink.dl_columns;
